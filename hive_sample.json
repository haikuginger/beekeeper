{
    "name": "SystemName",
    "root": "url_here",
    "versioning": {
        "version": 8,
        "previousVersions": [
            {
                "version": 7,
                "location": "http://path.to/previous/hive",
                "expires": "2015-12-31T12:00:00Z"
            }
        ]
    },
    "variables": {
        "Authorization": {
            "type": "header"
        },

        "subdomain": {
            "type": "url_replacement",
            "value": "my_subdomain"
        },

        "apikey": {
            "type": "url_param"
        }
    },
    "endpoints": {
        "EndpointOne": {
            "path": "url_extension_here",
            "methods": [
                "GET",
                "POST",
                "PUT"
            ],
            "variables": {
                //Same as the variables object on the API
            }
        }
        //Insert as many more Endpoint objects as you want
    },
    "objects": {
        "ObjectOne": {
            "description": "Short description string",
            "actions": {
                "create": {
                    "endpoint": "EndpointOne",
                    "method": "POST"
                },
                "update": {
                    "endpoint": "EndpointTwo",
                    "method": "PUT"
                },
                "get": {
                    "endpoint": "EndpointTwo",
                    "method": "GET"
                },
                "list": {
                    "endpoint": "EndpointThree",
                    "method": "GET"
                }
            }
        }
    }
}